{
  "name": "@creativecommons/cc-global-components",
  "version": "0.2.1",
  "description": "Global components used across Creative Commons web properties.",
  "keywords": [
    "Vue.js",
    "component library"
  ],
  "homepage": "https://opensource.creativecommons.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/creativecommons/cc-global-components.git"
  },
  "bugs": {
    "url": "https://github.com/creativecommons/cc-global-components"
  },
  "license": "MIT",
  "main": "dist/cc-globals.ssr.js",
  "browser": "dist/cc-globals.esm.js",
  "module": "dist/cc-globals.esm.js",
  "unpkg": "dist/cc-globals.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/babel-preset-app": "^4.5.15",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.11",
    "@vuepress/plugin-register-components": "^2.0.0-beta.29",
    "axios": "^0.24.0",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.0.5",
    "vuepress": "^2.0.0-beta.29"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  }
}
